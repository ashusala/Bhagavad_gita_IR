    "# getting the queries\n",
    "df = pd.read_csv('queries_syn.csv')\n",
    "queries = df['Verse'].to_list()\n",
    "# pre-processing the queries\n",
    "queries = [query.rstrip().lower().translate(str.maketrans('','',string.punctuation)) for query in queries]"
   ]
  },
